---
title: "{{ homer.title }}"
subtitle: "{{ homer.subtitle }}"
logo: "logo.png"

header: true
footer: false
connectivityCheck: false

theme: default
colors:
  light:
    highlight-primary: "#3367d6"
    highlight-secondary: "#4285f4"
    highlight-hover: "#5a95f5"
    background: "#f5f5f5"
    card-background: "#ffffff"
    text: "#363636"
    text-header: "#ffffff"
    text-title: "#303030"
    text-subtitle: "#424242"
    card-shadow: rgba(0, 0, 0, 0.1)
    link-hover: "#363636"
  dark:
    highlight-primary: "#3367d6"
    highlight-secondary: "#4285f4"
    highlight-hover: "#5a95f5"
    background: "#131313"
    card-background: "#2b2b2b"
    text: "#eaeaea"
    text-header: "#ffffff"
    text-title: "#fafafa"
    text-subtitle: "#f5f5f5"
    card-shadow: rgba(0, 0, 0, 0.4)
    link-hover: "#ffdd57"

links:
  - name: "SEEDbox"
    icon: "fab fa-github"
    url: "https://github.com/Pro-Tweaker/SEEDbox"
    target: "_blank"
  - name: "r/selfhosted"
    icon: "fab fa-reddit"
    url: "https://www.reddit.com/r/selfhosted/"
    target: "_blank"


services:
{% for service in homer.services %}
  - name: {{ service.name }}
    icon: {{ service.icon }}
{% endfor %}